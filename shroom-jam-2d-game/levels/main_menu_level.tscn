[sub_resource type="GDScript" id="GDScript_a0bpi"]
script/source = "# main_menu_button.gd
extends Button

@export var forest_scene: String = \"res://levels/forest_level.tscn\"

# Auto-find or auto-create everything — NO MORE ERRORS!
@onready var canvas_layer: CanvasLayer = get_parent()
@onready var animation_player: AnimationPlayer = canvas_layer.get_node_or_null(\"AnimationPlayer\")
@onready var color_rect: ColorRect = canvas_layer.get_node_or_null(\"fade_transition\")


func _on_pressed() -> void:
	disabled = true
	color_rect.visible = true
	animation_player.play(\"fade\")
	await animation_player.animation_finished
	get_tree().change_scene_to_file(forest_scene)
"

